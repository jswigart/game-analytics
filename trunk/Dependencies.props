    <SHA1>$(DEPENDENCIES)\smallsha1</SHA1>
    <LIBCURL>$(DEPENDENCIES)\libcurl\include</LIBCURL>
    <MD5>$(DEPENDENCIES)\md5</MD5>
    <SQLITE>$(DEPENDENCIES)\sqlite</SQLITE>
    <ENET>$(DEPENDENCIES)\enet-1.3.12</ENET>
    <PROTOBUF>D:\Sourcecode\omnibot\trunk\Omnibot\dependencies\protobuf\</PROTOBUF>
    <BOOST>D:\Sourcecode\omnibot\trunk\libs\boost\boost_1_57_0</BOOST>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)build/$(Configuration)/</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
